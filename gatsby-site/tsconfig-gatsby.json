{
  "compilerOptions": {
    "noImplicitAny": true,
    "module": "commonjs",
    "strict": true,
    "noEmit": false,
    "jsx": "react",
    "target": "esnext",
    "noImplicitReturns": true,
    "esModuleInterop": true,
    "rootDir": "./ts-configs",
    "outDir": "./"
  },
  "exclude": ["./node_modules", "./public", "./.cache", "./src/**/*"],
  "include": ["ts-configs/**/*"]
}
