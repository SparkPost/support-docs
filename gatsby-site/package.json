{
  "name": "support-documents",
  "version": "1.0.0",
  "private": true,
  "description": "Support Documents",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "build": "tsc -p ./tsconfig-gatsby.json && gatsby build",
    "develop": "tsc -p ./tsconfig-gatsby.json && gatsby develop",
    "serve": "tsc -p ./tsconfig-gatsby.json && gatsby serve",
    "clean": "tsc -p ./tsconfig-gatsby.json && gatsby clean",
    "type-check": "tsc --noEmit --pretty",
    "lint": "eslint . --ignore-path .gitignore --ext .ts,.tsx,.js",
    "test": "npm run type-check && npm run lint"
  },
  "sideEffects": false,
  "dependencies": {
    "@mdx-js/mdx": "^1.6.22",
