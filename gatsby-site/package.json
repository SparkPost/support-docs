    "@sparkpost/design-tokens": "^5.1.5",
    "@sparkpost/matchbox": "^5.1.5",
    "@sparkpost/matchbox-icons": "^5.1.5",
    "babel-plugin-styled-components": "^1.12.0",
    "gatsby": "^3.6.1",
    "gatsby-plugin-gatsby-cloud": "^2.6.1",
    "gatsby-plugin-google-analytics": "^3.7.1",
    "gatsby-plugin-image": "^1.7.1",
    "gatsby-plugin-manifest": "^3.7.1",
    "gatsby-plugin-mdx": "^2.7.1",
    "gatsby-plugin-react-helmet": "^4.7.1",
    "gatsby-plugin-sharp": "^3.7.1",
    "gatsby-plugin-sitemap": "^4.3.1",
